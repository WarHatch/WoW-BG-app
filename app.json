{
  "expo": {
    "name": "WoW: Boardgame manager",
    "slug": "WoWBG-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "primaryColor": "#2f212f",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.warhatch.WoWBGmanager"
    },
    "android": {
      "package": "com.warhatch.WoWBGmanager"
    },
    "description": "A react-native app for World of Warcraft: the Board Game character management"
  }
}